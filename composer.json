{
    "name": "ccmelas/laravel-sharenet",
    "description": "A Laravel Package for Sharenet",
    "keywords": [
        "laravel", "notifications", "custom notifications",
        "sharenet", "SMS", "messaging"
    ],
    "require": {
        "illuminate/support": "^5.5",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "mockery/mockery": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Chiemela Chinedum",
            "email": "chiemelachinedum@gmail.com",
            "homepage": "https://github.com/ccmelas",
            "role": "developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Melas\\Sharenet\\": "/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Melas\\Sharenet\\Test\\": "/tests"
        }
    },
    "scripts": {
        "test": "../../../vendor/bin/phpunit"
    },
    "minimum-stability": "stable"
}
